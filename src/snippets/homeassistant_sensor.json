{
  "(binary) sensor template": {
    "prefix": "sensor-template",
    "body": [
      "- platform: template",
      "  sensors:",
      "    ${1:sensor name}:",
      "      friendly_name: \"${2:sensor friendly name}\"",
      "      value_template: \"{{ ${3:sensor value template} }}\""
    ],
    "description": "Add a Home Assistant template (binary) sensor"
  }
}
